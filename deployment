---
apiVersion: v1
kind: Pod
metadata:
   name: mypod
   labels:
      app: swiggy
spec:
  containers:
     - name: cont-1
       image: shaikmustafa/cycle
       ports:
        - containerPort: 80
---
apiVersion: v1
kind: Service
metadata:
  name: new
spec:
  type: NodePort
  selector:
    app: swiggy
  ports:
   - port: 80
     targetPort: 80
